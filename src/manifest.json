{
  "manifest_version": 2,
  "name": "Example with Parcel",
  "description": "Example Chrome Extension",
  "version": "0.0.1",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
